<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	<bean class="org.apache.wink.spring.Registrar">
		<!-- Use "classes" to register things that don't   -->
		<!-- need to have dependencies injected by Spring. -->
		<!--
		<property name="classes">
			<set value-type="java.lang.Class">
				<value>${package}.ExampleResource</value>
			</set>
		</property>
		-->
		<!-- Use "instances" to register spring beans. -->
		<property name="instances">
			<set>
				<ref bean="${package}.ExampleResource" />
			</set>
		</property>
	</bean>

	<bean class="${package}.ExampleResource">
		<property name="exampleLogic" ref="${package}.ExampleLogic" />
	</bean>
</beans>
